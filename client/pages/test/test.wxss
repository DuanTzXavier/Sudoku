.table {
  border: 1px solid darkgray;
  width: 100%-1px;
  height: 750rpx
}
.tr {
  display: flex;
  width: 100%;
  height: 11.111%;
}
.td {
    width:100%;
    height: 100%;
    background: bisque;
    border: 1px solid darkgray;
    text-align: center;
    align-content: center;
}
.bg-g{
  background: #E6F3F9;
}
.center{
    width: 100%;
    background: #FFF;
    display: flex;
    height: auto;
    padding: 15rpx 0 0 30rpx;
    flex-direction: column;
    align-items: flex-start;
    box-sizing: border-box;
}